4

console.group('stringLiterals')
console.log("\"string1\"\n'string2'\n`string3`")
console.groupEnd()

console.group('numberLiterals')
console.log("Десятичные: 20,12,.4\nЭкспоненциальная форма:3.6e10, 2.4e-10 \nШестнадцатеричные:0x245, 00X7B\nВосьмеричные:0324,0235")
console.groupEnd()

console.group('booleanLiterals')
console.log("false\ntrue")
console.groupEnd()

5

console.time()

console.group('literalsGroups')

console.group('stringLiterals')
console.log("\"string1\"\n'string2'\n``string3``")
console.groupEnd()

console.group('numberLiterals')
console.log("Десятичные: 20,12,.4\nЭкспоненциальная форма:3.6e10, 2.4e-10 \nШестнадцатеричные:0x245, 00X7B\nВосьмеричные:0324,0235")
console.groupEnd()

console.group('booleanLiterals')
console.log("false\ntrue")
console.groupEnd()

console.group('otherLiterals')
console.log("null\nObject -{}\nArray - []")
console.groupEnd()



console.groupEnd()

console.timeEnd()

