8

function info () {
	var name = prompt('Имя?', ''),
		surname = prompt('Фамилия', ''),
		age = prompt('Сколько тебе лет?', ''),
		male = prompt('Пол? ', '');
	console.info(name + ' ' + surname + ', ' + age + ' лет, ' + male)
}
info()


